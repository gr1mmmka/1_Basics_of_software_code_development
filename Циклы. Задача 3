public class firstPartTask1 {
    public static void main(String[] args) {
        // Циклы. Задача 2. Найти сумму квадратов первых 100 чисел

        int sumSqr = 0;
        for (int i=1; i<=100; i++) {
            sumSqr += Math.pow(i,2);
        }
        System.out.println("Сумма первых ста числе равна " + sumSqr);
    }
}
