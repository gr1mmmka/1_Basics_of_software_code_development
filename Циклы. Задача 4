public class firstPartTask1 {
    public static void main(String[] args) {
        // Циклы. Задача 4. Найти произведение квадратов от 1 до 200

        double sumSqr = 1;
        for (int i=1; i<=200; i++) {
            sumSqr *= Math.pow(i,2);
        }
        System.out.println("Сумма первых двухста числе равна " + sumSqr);
    }
}
